{
  "name": "@emirayka/parcom",
  "version": "0.0.3",
  "description": "Parser combinator",
  "keywords": [
    "library",
    "typescript",
    "parser combinator"
  ],
  "homepage": "https://github.com/emirayka/parcom",
  "bugs": {
    "url": "https://github.com/emirayka/parcom/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emirayka/parcom.git"
  },
  "license": "MIT",
  "author": "emirayka emirayka@protonmail.com",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "dist/index.cjs.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run test && npm run clean",
    "build": "npm run build:js",
    "build:js": "rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint 'src/**/*' --fix",
    "prerelease": "npm run build",
    "release": "npx standard-version",
    "postrelease": "git push --follow-tags",
    "start": "rollup -c -w",
    "pretest": "npm run lint",
    "test": "jest",
    "test:cov": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch --notify",
    "types:check": "tsc --noEmit",
    "types:watch": "npm run types:check -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@wessberg/rollup-plugin-ts": "^1.2.28",
    "eslint": "^7.4.0",
    "eslint-plugin-jest": "^23.18.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@emirayka/option-result": "0.0.3"
  }
